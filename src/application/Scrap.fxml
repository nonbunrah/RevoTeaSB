<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.RevoTeaController">
   <children>
      <Label fx:id="labelId" onDragDetected="#newButtonClick" text="Label">
         <font>
            <Font size="39.0" />
         </font>
      </Label>
      <Button mnemonicParsing="false" onAction="#newButtonClick" text="Button" />
   </children>
</VBox>
